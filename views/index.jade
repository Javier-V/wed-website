        .class-thumb
          a(href='https://www.hilton.com/en/hotels/lcjdldi-doubletree-lodz/' target='_blank')
            img.img-fluid(src='images/doubleTree.jpg' alt='DoubleTree by Hilton Lodz')
          .class-info
            h3.mb-1 DoubleTree Hilton
            span.class-price $$$
            
      .mt-5.mt-lg-0.col-lg-4.col-md-6.col-12(data-aos='fade-up' data-aos-delay='600')
        .class-thumb
          a(href='https://www.hilton.com/en/hotels/lcjdldi-doubletree-lodz/' target='_blank')
            img.img-fluid(src='images/doubleTree.jpg' alt='DoubleTree by Hilton Lodz')
          .class-info
            h3.mb-1 DoubleTree Hilton
            span.class-price $$$
//- SCHEDULE
section#schedule.schedule.section
  .container
    .row
      .col-lg-12.col-12.text-center
        h6(data-aos='fade-up') #{__('schedule_description1')}
        h2.text-white(data-aos='fade-up' data-aos-delay='200') #{__('schedule_description2')}
      .col-lg-12.py-5.col-md-12.col-12
        table.table.table-bordered.table-responsive.schedule-table(data-aos='fade-up' data-aos-delay='300')
          thead.thead-light
            th
              i.fa.fa-calendar
            th
          tbody
            tr
              td
                small 17:00
              td
                strong #{__('Ceremony and Wishes')}
              
            tr
              td
                small 17:50
              td
                strong #{__('Speeches')}
              
            tr
              td
                small 18:00
              td
                strong #{__('Lunch')}
              
            tr
              td
                small 19:30
              td
                strong #{__('First Dance')}

            tr
              td 
                small 22:30
              td 
                strong #{__('Wedding dish surprise')}
                span #{__('Delicious')}

            tr
              td 
                small 00:00
              td 
                strong #{__('Wedding cake')}

            tr
              td 
                small 02:00
              td 
                strong #{__('Late Dinner 1')}

            tr
              td 
                small 04:30
              td 
                strong #{__('Late Dinner 2')}            
//- LOCATION
section#location.contact.section
  .container
    .row
      .mx-auto.mt-4.mt-lg-0.mt-md-0.col-lg-12.col-md-12.col-12
        h2.mb-4(data-aos='fade-up' data-aos-delay='600')
          |#{__('The location')}
        p(data-aos='fade-up' data-aos-delay='800')
          i.fa.fa-map-marker.mr-1
          |  Krzemieniecka 2A, Łódź, Poland
        p(data-aos='fade-up' data-aos-delay='900')
          i.mr-1 #{__('There is a private parking in the building')}
        .google-map(data-aos='fade-up' data-aos-delay='900')
          iframe(src='https://maps.google.com/maps?q=Krzemieniecka+2A,+90-001+%C5%81%C3%B3d%C5%BA,+Poland&t=&z=13&ie=UTF8&iwloc=&output=embed' width='1920' height='250' frameborder='0' style='border:0;' allowfullscreen='')
//- FOOTER
footer.site-footer
  .container
    .row
      .ml-auto.col-lg-4.col-md-5
        p.copyright-text
          | Copyright &copy; 2020 J. Valero
          br
          | Design based on: 
          a(href='https://www.tooplate.com') Tooplate
      .d-flex.justify-content-center.mx-auto.col-lg-5.col-md-7.col-12
