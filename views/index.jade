          |  010-020-0840
// Modal
#membershipForm.modal.fade(tabindex='-1' role='dialog' aria-labelledby='membershipFormLabel' aria-hidden='true')
  .modal-dialog(role='document')
    .modal-content
      .modal-header
        h2#membershipFormLabel.modal-title Membership Form
        button.close(type='button' data-dismiss='modal' aria-label='Close')
          span(aria-hidden='true') &times;
      .modal-body
        form.membership-form.webform(role='form')
          input.form-control(type='text' name='cf-name' placeholder='John Doe')
          input.form-control(type='email' name='cf-email' placeholder='Johndoe@gmail.com')
          input.form-control(type='tel' name='cf-phone' placeholder='123-456-7890' pattern='[0-9]{3}-[0-9]{3}-[0-9]{4}' required='')
          textarea.form-control(rows='3' name='cf-message' placeholder='Additional Message')
          button#submit-button.form-control(type='submit' name='submit') Submit Button
          .custom-control.custom-checkbox
            input#signup-agree.custom-control-input(type='checkbox')
            label.custom-control-label.text-small.text-muted(for='signup-agree')
              | I agree to the 
              a(href='#') Terms &amp;Conditions
      .modal-footer
